{% extends 'base.twig' %}

{% block main %}

    {% for element in musique %}
    <div class="presentation">
        <img src="{{ element.album_pochette }}" alt="">
        <h1>{{element.artiste_nom}}</h1>
        <p>{{element.album_titre}}</p>
        <p>{{element.album_description}}</p>
    </div>

    <div class="liste-musique">
        <img src="{{ element.album_pochette }}" alt="">
        <p>{{ element.musique_nom }}</p>
        <a href="artiste.php?id={{ element.id_artiste }}">{{ element.artiste_nom }}</a>
        <i class=""></i>
        <p>{{ element.musique_duree }}</p>
    </div>
    {% endfor %}

{% endblock %}